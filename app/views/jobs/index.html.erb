All Jobs for <strong><%= @company.name %></strong>

<% @jobs.each do |job| %>
  <ul>
  <%= job.title %> at <%= job.company.name %>
  <%= link_to "Edit #{job.title}", edit_company_job_path(@company, job) %>
  <%= link_to "Delete #{job.title}", company_job_path(@company, job), method: "delete" %>
  </ul>
<% end %>
