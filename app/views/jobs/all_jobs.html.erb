<% @jobs.each do |job| %>
  <ul>
  <li>Level of Interest:   <%= job.level_of_interest %> for
  <%= link_to job.title, company_job_path(job.company, job) %> at <%= job.company.name %>
  <br>
  <%= link_to edit_company_job_path(job.company, job) do %>
  <button type="button" class="btn btn-success" aria-haspopup="true" aria-expanded="false">
    <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
  </button>
  <% end %>
  <%= link_to company_job_path(job.company, job), method: "delete" do %>
  <button type="button" class="btn btn-danger" aria-haspopup="true" aria-expanded="false">
    <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
  </button>
  <% end %>
</li>
  </ul>
<% end %>
